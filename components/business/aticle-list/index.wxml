<view class=""
      wx:for="{{list}}" wx:key="index"
      bindtap="go"
      data-url="article/detail"
      data-id="{{item.id}}"
>
    <view class="y-flex y-align-between y-vertical-center">
        <view class="y-p-r-30 ">
            <view class="y-flex y-vertical-center y-line-height-180 y-font-l  ellipsis">
                {{item.title}} {{item.title}} {{item.title}}
            </view>
        </view>
        <van-image src="{{item.img}}" fit="widthFix" width="120rpx"></van-image>
    </view>
    <view class="y-col-9">
        <van-divider custom-style="padding:0;margin:20rpx 0"/>
    </view>
</view>
